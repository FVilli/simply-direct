# Creazione di un file .gitignore per un progetto fullstack TypeScript (Angular + NestJS)
gitignore_content = """
# Node
node_modules/
dist/
build/
tmp/
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Logs
logs/
*.log

# Environment
.env
.env.local
.env.*.local

# TypeScript
*.tsbuildinfo

# Angular
.angular/
.angular/cache/
out-tsc/
coverage/

# IDEs
.vscode/
.idea/

# System files
.DS_Store
Thumbs.db

# GitHub
*.tgz

# Example builds
examples/**/dist/
"""

gitignore_path = Path("/mnt/data/.gitignore")
gitignore_path.write_text(gitignore_content)

gitignore_path.name
